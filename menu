<script>
   jQuery(function() {
      var navBox = jQuery("#headbar");
      navBox.hide();
      var TargetPos = 80;
      jQuery(window).scroll( function() {
         var ScrollPos = jQuery(window).scrollTop();
         if( ScrollPos > TargetPos ) {
            navBox.fadeIn();
         }
         else {
            navBox.fadeOut();
         }
      });
   });
</script>
<script type="text/javascript">
	jQuery(function(jQuery){
	  //下方向から出現
	jQuery('.slide-bottom').on('inview', function(event, isInView, visiblePartX, visiblePartY) {
		console.log(isInView);
		if(isInView){
			jQuery(this).stop().addClass('slide-stop');
		}
		else{
			jQuery(this).stop().removeClass('slide-stop');
		}
	});
	});
</script>
